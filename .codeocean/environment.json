{
	"version": 1,
	"base_image": "codeocean/mambaforge3:23.1.0-4-python3.10.12-ubuntu22.04",
	"args": [
		"AWS_ACCESS_KEY_ID",
		"AWS_DEFAULT_REGION",
		"AWS_SECRET_ACCESS_KEY"
	],
	"post_install": true,
	"options": {
		"registry_host_arg": true,
		"git_ask_pass": true
	},
	"installers": {
		"pip3": {
			"packages": [
				{
					"name": "aind-data-access-api",
					"version": "1.2.1"
				},
				{
					"name": "hdmf",
					"version": "3.14.6"
				},
				{
					"name": "hdmf-zarr",
					"version": "0.11.1"
				},
				{
					"name": "matplotlib",
					"version": "3.10.1"
				},
				{
					"name": "numpy",
					"version": "2.2.5"
				},
				{
					"name": "nwbwidgets",
					"version": "0.11.3"
				},
				{
					"name": "pandas",
					"version": "2.2.3"
				},
				{
					"name": "pynwb",
					"version": "3.0.0"
				}
			],
			"options": {},
			"pre_install_options": {}
		}
	}
}